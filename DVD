package mckennajamesproject1;

/**
 *
 * @author jamck
 */
public class DVD {
    private String title;
    private int quantity;
    private int onePiece250;
    private int onePiece500;
    private int onePiece750;
    
     // Getter for title
    public String getTitle() {
        return title;
    }

    // Setter for title
    public void setTitle(String title) {
        this.title = title;
    }

    // Getter for quantity
    public int getQuantity() {
        return quantity;
    }

    // Setter for quantity
    public void setQuantity(int quantity) {
        this.quantity = quantity;
    }

    // Getter for onePiece250
    public int getOnePiece250() {
        return onePiece250;
    }

    // Setter for onePiece250
    public void setOnePiece250(int onePiece250) {
        this.onePiece250 = onePiece250;
    }

    // Getter for onePiece500
    public int getOnePiece500() {
        return onePiece500;
    }

    // Setter for onePiece500
    public void setOnePiece500(int onePiece500) {
        this.onePiece500 = onePiece500;
    }

    // Getter for onePiece750
    public int getOnePiece750() {
        return onePiece750;
    }

    // Setter for onePiece750
    public void setOnePiece750(int onePiece750) {
        this.onePiece750 = onePiece750;
    }
    
    public DVD(String title, int quantity) {
        this.title = title;
        this.quantity = quantity;
    }
    
}
