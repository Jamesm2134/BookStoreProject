package mckennajamesproject1;

/**
 *
 * @author jamck
 */

////////////////////////////////////////////////////////////////////////
/*

cd class creates the cd atributes for the inventory class to popualte.

*/
////////////////////////////////////////////////////////////////////////
public class CD {
    private String title;
    private int number;
    private int quantity;
    private double price;
    private String type;
    
    // getters and setters
    public int getNumber(){
        return number;
    }
    public void setNumber(int number){
        this.number = number;
    }
    
    public String getType() {
        return type;
    }
    public void setType(String type) {
        this.type = type;
    }
    
    public String getTitle() {
        return title;
    }
    public void setTitle(String title) {
        this.title = title;
    }
    public int getQuantity() {
        return quantity;
    }
    public void setQuantity(int quantity) {
        this.quantity = quantity;
    }
    public double getPrice() {
        return price;
    }
    public void setPrice(double price) {
        this.price = price;
    }
    
    // cd constructor
    public CD(int number, String title, int quantity,double price, String type) {
        this.number = number;
        this.title = title;
        this.quantity = quantity;
        this.price = price;
        this.type = type;
    }

    
}
